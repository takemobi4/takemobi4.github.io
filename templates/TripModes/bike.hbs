<div class="mode-result row">
    <div class="mode-heading col-xs-12">
         <strong>{{getTime checkpoints.0.latest-time}}</strong> Bike to {{endLoc}}
    </div>
    <div class="mode-content bike col-xs-12">
        <div class="mode-icon">
            {{{iconBlock type}}}
        </div>
        <div class="mode-description">
            <p>{{round length}} miles - {{noDecimal lowerbound}} mins - {{noDecimal calories.amount}} calories </p>
        </div>
    </div>
</div>